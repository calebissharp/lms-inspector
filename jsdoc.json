{
  "source": {
    "include": ["src/", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "destination": "docs",
    "recurse": true,
    "verbose": true
  }
}
